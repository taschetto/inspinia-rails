<% content_for :title, "Editar #{@organization.name}" %>
<% breadcrumb :edit_organization, @organization %>
<% content_for :actions do %>
<%= link_to "Atualizar Organização", "#", class: "btn btn-primary pull-right", onclick: '$(".edit_organization").submit()' %>
<% end %>

<%= form_for @organization do |f| %>
  <%= render partial: "form", locals: { f: f } %>
<% end %>