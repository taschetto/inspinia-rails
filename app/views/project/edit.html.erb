<% content_for :title, "Editar #{@project.name}" %>
<% breadcrumb :edit_project, @project %>
<% content_for :actions do %>
<%= link_to "Atualizar Projeto", "#", class: "btn btn-primary pull-right", onclick: '$(".edit_project").submit()' %>
<% end %>

<%= form_for @project do |f| %>
  <%= render partial: "form", locals: { f: f } %>
<% end %>