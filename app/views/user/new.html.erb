<% content_for :title, "Novo Usuário" %>
<% breadcrumb :new_user %>
<% content_for :actions do %>
<%= link_to "Salvar Usuário", "#", class: "btn btn-primary pull-right", onclick: '$(".new_user").submit()' %>
<% end %>

<%= form_for @user, url: user_index_path, id: "user" do |f| %>
<%= render partial: "form", object: @user, as: 'user' %>
<% end %>    
