        <h3>Fator 7 Tecnologia da Informação</h3>
        <%= devise_error_messages! %>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name), class: "m-t", role: "form") do |f| %>
          <div class="form-group">
            <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Usuário" %>
          </div>
          <div class="form-group">
            <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Senha" %>
          </div>
          <% if devise_mapping.rememberable? -%>
          <div class="form-group">
            <%= f.check_box :remember_me, class: "i-checks" %> <%= f.label :remember_me %>
          </div>
          <% end %>
          <%= f.submit "Entrar", class: "btn btn-primary block full-width m-b" %>
          <%= render "devise/shared/links" %>
        <% end %>

